<div class="row marketing">
  <div class="col-lg-12">
    <h4><%= t(".page_title", query: @query) %></h4>

    <% if @venues.empty? %>

      <p><%= t(".no_results") %></p>
      <p><%= link_to t(".go_back"), root_path %></p>

    <% else %>

      <ul>
      <% @venues.each do |venue| %>
        <li><%= link_to venue.name, venue %></li>
      <% end %>
      </ul>

    <% end %>
  </div>
</div>